const
 max                         = 50000;

var
 fe,fs                       : text;
 n                           : longint;
 mk                          : array[0..max+1] of boolean;

procedure open;
begin
 assign(fe,'dinner.in'); reset(fe);
 assign(fs,'dinner.out'); rewrite(fs);
 readln(fe,n);
end;

procedure work;
var
 i,aux                     : longint;
begin
 fillchar(mk,n,false);
 mk[1]:=false; mk[0]:=true; mk[max+1]:=false;
 for i:=1 to n do
   begin
    readln(fe,aux);
    if mk[aux] then
      writeln(fs,aux+1)
    else
      begin
       if not mk[aux-1] then
         begin
          writeln(fs,aux-1);
          mk[aux-1]:=true;
         end
       else
        writeln(fs,aux);
      end;
    mk[aux]:=true;
   end;
 close(fs);
end;

begin
 open;
 work;
end.